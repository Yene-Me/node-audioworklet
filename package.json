{
	"name": "audioworklet",
	"version": "0.2.1",
	"description": "Stream audio generated in realtime by a function to the speaker",
	"main": "src/index.js",
	"types": "src/index.d.ts",
	"scripts": {
		"build": "cmake-js compile",
		"install": "prebuild-install || npm run rebuild",
		"deploy": "np --no-tests",
		"build-binaries": "prebuild -r napi -t 4 --backend cmake-js --include-regex \"^.*\\.(node|dylib|dll|so(\\.[0-9])?)$\" --verbose",
		"build-binaries-and-upload": "prebuild -r napi -t 4 --backend cmake-js --include-regex \"^.*\\.(node|dylib|dll|so(\\.[0-9])?)$\" --verbose -u $GITHUB_TOKEN"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/geekuillaume/node-audioworklet.git"
	},
	"author": "Guillaume Besson",
	"license": "MIT",
	"homepage": "https://github.com/geekuillaume/node-audioworklet#readme",
	"dependencies": {
		"bindings": "^1.5.0",
		"cmake-js": "^6.0.0",
		"node-addon-api": "^2.0.0",
		"prebuild-install": "^5.3.3"
	},
	"devDependencies": {
		"@types/node": "^12.12.17",
		"np": "^5.2.1",
		"prebuild": "^9.1.1",
		"run-script-os": "^1.0.7",
		"simple-git": "^1.129.0"
	},
	"binary": {
		"napi_versions": [4]
	}
}
